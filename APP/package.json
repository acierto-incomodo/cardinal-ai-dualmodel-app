{
  "name": "cardinal-ai-dualmodel-app",
  "version": "1.128.1450",
  "description": "CardinalAI MultiModel App es una aplicación de escritorio que permite a los usuarios interactuar con múltiples modelos de IA de CardinalAI (de StormGamesStudios), proporcionando una interfaz fácil de usar para generar y gestionar contenido generado por inteligencia artificial.",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win",
    "publish": "electron-builder --win --publish always",
    "build": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "install": "npm install",
    "directInstallBuild": "npm run install && npm run build",
    "directStart": "npm run install && npm start",
    "dist:linux": "electron-builder --linux",
    "build:linux": "electron-builder --linux --publish never",
    "dist:snap": "electron-builder --linux snap",
    "build:snap": "snapcraft pack"
  },
  "dependencies": {
    "electron-store": "^8.2.0"
  },
  "devDependencies": {
    "electron": "^25.9.8",
    "electron-builder": "^26.0.12"
  },
  "homepage": "https://github.com/acierto-incomodo/cardinal-ai-dualmodel-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/acierto-incomodo/cardinal-ai-dualmodel-app.git"
  },
  "author": {
    "name": "StormGamesStudios",
    "email": "meliodas.aitor@gmail.com"
  },
  "keywords": [
    "electron",
    "desktop",
    "application",
    "AI",
    "CardinalAI",
    "multi-model"
  ],
  "bugs": {
    "url": "https://github.com/acierto-incomodo/cardinal-ai-dualmodel-app/issues"
  },
  "license": "FREEWARE",
  "build": {
    "appId": "com.stormgamesstudios.cardinalai",
    "productName": "Cardinal AI MultiModel",
    "extraResources": [
      {
        "from": "icons/icon.ico",
        "to": "icons/icon.ico",
        "filter": "**/*"
      },
      {
        "from": "icons/icon.ico",
        "to": "icon.ico",
        "filter": "**/*"
      },
      {
        "from": "LICENSE.txt",
        "to": "license/LICENSE.txt",
        "filter": "**/*"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        }
      ],
      "icon": "icons/icon.ico",
      "signtoolOptions": {
        "publisherName": "StormGamesStudios"
      }
    },
    "linux": {
      "target": ["deb", "rpm", "AppImage", "snap"],
      "icon": "icons",
      "category": "Utility",
      "maintainer": "StormGamesStudios <meliodas.aitor@gmail.com>",
      "vendor": "StormGamesStudios",
      "desktop": {
        "entry": {
          "Type": "Application",
          "Name": "Cardinal AI MultiModel",
          "Comment": "CardinalAI MultiModel Application",
          "Categories": "Utility;Development;",
          "Terminal": "false"
        }
      }
    },
    "snap": {
      "confinement": "strict",
      "grade": "stable",
      "plugs": [
        "network",
        "home",
        "x11",
        "desktop",
        "wayland",
        "opengl",
        "audio-playback"
      ],
      "base": "core22"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": false,
      "license": "LICENSE.txt"
    }
  }
}
